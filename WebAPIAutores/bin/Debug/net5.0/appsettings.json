{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "ConnectionStrings": {
    "defaultConnection": "Data Source=URL_Azure_Server;Initial Catalog=CourseWebApis;MyCredentials"
  },
  "LimitRequest": {
    "FreeRequestForDay": 20,
    "WhiteListRoutes": [ "api/v1/Accounts", "api/KeysAPI", "favicon.ico", "api/DomainRestriction", "api/IpRestriction", "api/invoice" ]
  },
  "AllowedHosts": "*"
}
